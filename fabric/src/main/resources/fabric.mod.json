{
	"schemaVersion": 1,
	"id": "easiersneak",
	"version": "${version}",
	"name": "EasierSneak",
	"description": "A mod that allows you to toggle sneak mode with Ctrl+Shift, making sneaking easier and more convenient.",
	"authors": [
		"TiesToetToet"
	],
	"contact": {
		"homepage": "https://github.com/TiesToetToet/EasierSneak",
		"sources": "https://github.com/TiesToetToet/EasierSneak"
	},
	"license": "MIT",
	"icon": "assets/easiersneak/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.tiesttoettoet.easier_sneak.EasierSneak"
		],
		"client": [
			"com.tiesttoettoet.easier_sneak.EasierSneakClient"
		],
		"fabric-datagen": [
			"com.tiesttoettoet.easier_sneak.EasierSneakDataGenerator"
		]
	},
	"mixins": [
		"easiersneak.mixins.json",
		{
			"config": "easiersneak.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	}
}